<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="409dp"
    android:layout_height="match_parent"
    android:background="#EDF2FA"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="#239195"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#070767"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="50dp"
                android:layout_height="60dp"
                android:background="#070767"
                app:srcCompat="@drawable/paytm" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="54dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/person" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="137dp"
                android:layout_height="match_parent" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="46dp"
                android:layout_height="60dp"
                android:paddingTop="6dp"
                app:srcCompat="@drawable/notification" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="40dp"
                android:layout_height="60dp"
                android:paddingTop="5dp"
                app:srcCompat="@drawable/qrcode" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="83dp"
                android:layout_height="match_parent"
                android:paddingTop="5dp"
                app:srcCompat="@drawable/help" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="408dp"
        android:layout_height="34dp"
        android:layout_marginTop="24dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:text="Recharge and Bill Payments"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="408dp"
        android:layout_height="92dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="24dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/mobile" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/recharge" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/creditcard" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/phone" />

            <ImageView
                android:id="@+id/imageView28"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/rent" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:paddingTop="5dp">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Mobile Prepaid"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="83dp"
                android:layout_height="40dp"
                android:text="Mobile Postpaid"
                android:textAlignment="center"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="Credit Card"
                android:textAlignment="center"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Landline"
                android:textAlignment="center"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="House Rent"
                android:textAlignment="center"
                android:textColor="#000000" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="408dp"
        android:layout_height="34dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="17dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Travel and Ticket Booking"
            android:textColor="#F2120000"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="410dp"
        android:layout_height="85dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="25dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/cab" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/bus" />

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/flight" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/car" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/train" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:paddingTop="5dp">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Cab"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Bus"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Flight"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Car"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="88dp"
                android:layout_height="40dp"
                android:text="Train"
                android:textAlignment="center"
                android:textColor="#120F0F" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="408dp"
        android:layout_height="34dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="16dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Shop on Paytm"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="409dp"
        android:layout_height="85dp"
        android:layout_marginTop="21dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/mobshop" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/clothes" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/grocery" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/footwear" />

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/watches" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="43dp"
            android:orientation="horizontal"
            android:paddingTop="5dp">

            <TextView
                android:id="@+id/textView21"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Mobiles"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Clothing"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Grocery"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="FootWear"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Watches"
                android:textAlignment="center"
                android:textColor="#090000" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="405dp"
        android:layout_height="34dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="1dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Insurance"
            android:textColor="#090808"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="85dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="24dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/corona" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/carin" />

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/bikee" />

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/health" />

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="80dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/guardian" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:paddingTop="5dp">

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:text="CoronaVirus"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Car"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Bike"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Health"
                android:textAlignment="center"
                android:textColor="#090000" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="Term Life"
                android:textAlignment="center"
                android:textColor="#090000" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>